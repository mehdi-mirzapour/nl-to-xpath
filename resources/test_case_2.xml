<html lang="en">
  <head>
    <title>Complex Test Page</title>
  </head>
  <body>
    <header class="main-header" data-section="navigation">
      <nav>
        <ul>
          <li><a href="/about-us" class="nav-link" data-test-id="about-link">About Us</a></li>
          <li><a href="/home" class="nav-link">Home</a></li>
          <li><a href="/about" class="nav-link">About Us (Secondary)</a></li>
        </ul>
      </nav>
    </header>
    <main>
      <section id="employee-section" class="content-section">
        <h2>Employee Management</h2>
        <div class="action-buttons" data-dynamic-id="emp-12345">
          <button class="btn primary" onclick="submit()">OK</button>
          <button class="btn secondary" onclick="cancel()">Cancel</button>
          <button class="btn hidden" style="display: none;">OK (Hidden)</button>
        </div>
        <div class="sub-section" data-section="employee-details">
          <button class="btn primary" onclick="confirm()">OK</button>
        </div>
      </section>
      <section id="form-section" class="content-section">
        <form class="user-form">
          <div class="form-group country-group" data-test-id="country-form">
            <label for="country-select">Country</label>
            <select id="country-select" name="country">
              <option value="usa">USA</option>
              <option value="canada">Canada</option>
            </select>
          </div>
          <div class="form-group">
            <label for="state-select">State</label>
            <select id="state-select" name="state">
              <option value="ca">California</option>
              <option value="ny">New York</option>
            </select>
          </div>
        </form>
      </section>
    </main>
    <footer>
      <a href="/contact" class="footer-link">Contact Us</a>
    </footer>
  </body>
</html>